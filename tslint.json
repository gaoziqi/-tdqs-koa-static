{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": false,
        "trailing-comma": false,
        "no-console": false,
        "no-namespace": false,
        "no-eval": false,
        "max-classes-per-file": false,
        "forin": false,
        "arrow-parens": false
    },
    "rulesDirectory": []
}